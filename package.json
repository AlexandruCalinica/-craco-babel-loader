{
  "name": "@craco/babel-loader",
  "version": "0.1.4",
  "description": "Rewire `babel-loader` loader in your `create-react-app` project using `craco`.",
  "source": "src/index.ts",
  "module": "dist/index.es.js",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "repository": "https://github.com/rjerue/craco-babel-loader",
  "author": {
    "name": "Ryan Jerue",
    "email": "ryan@jerue.org",
    "url": "http://jerue.org"
  },
  "license": "MIT",
  "eslintConfig": {
    "parser": "@babel/eslint-parser",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "extends": [
      "eslint:recommended"
    ]
  },
  "scripts": {
    "build": "microbundle --compress=false",
    "dev": "microbundle watch --compress=false",
    "pretty": "prettier --write --tab-width 2 'src/**/*.ts' 'test/**/*.ts'",
    "prepublish": "npm run lint && npm run test && npm run build",
    "lint": "eslint src test",
    "test": "echo 'run test'"
  },
  "keywords": [
    "babel-loader",
    "craco",
    "webpack",
    "create-react-app"
  ],
  "dependencies": {
    "@craco/craco": "^5.9.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "eslint": "^7.31.0",
    "microbundle": "^0.13.3",
    "prettier": "^2.3.2"
  }
}
